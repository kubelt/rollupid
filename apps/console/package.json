{
  "name": "@kubelt/apps.console",
  "version": "0.1.0",
  "private": true,
  "sideEffects": true,
  "scripts": {
    "storybook": "echo \"no storybook setup\"",
    "build-storybook": "echo \"no storybook setup\"",
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "run-s \"build:*\"",
    "build:css": "yarn run generate:css -- --minify",
    "build:remix": "remix build",
    "deploy": "wrangler publish",
    "deploy:dev": "wrangler publish --env dev",
    "dev:css": "yarn run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler dev --local --persist --env dev",
    "dev": "echo \"No dev for console. Please use start.\"",
    "start": "run-p \"dev:*\"",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@headlessui/react": "1.7.4",
    "@heroicons/react": "2.0.13",
    "@remix-run/cloudflare": "1.7.5",
    "@remix-run/cloudflare-workers": "1.7.5",
    "@remix-run/react": "1.7.3",
    "@tailwindcss/forms": "0.5.3",
    "bcryptjs": "2.4.3",
    "cross-env": "7.0.3",
    "ethers": "5.7.2",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "wagmi": "0.8.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.18.0",
    "@remix-run/dev": "1.7.5",
    "@remix-run/eslint-config": "1.7.5",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.27.0",
    "npm-run-all": "4.1.5",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4",
    "wrangler": "2.4.2",
    "yarn-run-all": "3.1.1"
  },
  "engines": {
    "node": ">=16.13"
  },
  "packageManager": "yarn@3.2.4"
}
