{
  "name": "passport",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "run-s \"build:*\"",
    "build:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "build:sass": "sass app/:app/ --no-source-map",
    "deploy": "wrangler publish",
    "dev:sass": "sass --watch app/:app/",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler dev --local --persist",
    "dev": "echo \"No dev for passport. Please use start.\"",
    "start": "run-p \"dev:*\""
  },
  "dependencies": {
    "@datadog/browser-rum": "4.19.0",
    "@datadog/datadog-api-client": "^1.3.0",
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "2.0.10",
    "@kubelt/threeid.themes.base-theme": "0.0.0-05450877644b41bf3d700e68e0515f99ba7b093d",
    "@remix-run/cloudflare": "^1.7.4",
    "@remix-run/cloudflare-workers": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "cross-env": "^7.0.3",
    "ethers": "5.7.0",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "graphql-tag": "2.12.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "wagmi": "0.6.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.1",
    "@remix-run/dev": "1.7.4",
    "@remix-run/eslint-config": "1.7.4",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "eslint": "8.23.1",
    "miniflare": "2.6.0",
    "sass": "1.54.7",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "wrangler": "2.1.15",
    "yarn-run-all": "3.1.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
