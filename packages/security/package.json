{
  "name": "@kubelt/security",
  "version": "0.1.0",
  "author": "Proof Zero Inc.",
  "description": "Platform security definitions and utilities",
  "private": true,
  "scripts": {
    "build:dev": "tsc",
    "docs": "run-s 'build:*' 'docs:*'",
    "docs:build": "api-extractor run --local",
    "format": "run-s format:src",
    "format:src": "prettier --write *.ts",
    "format:check": "run-s 'format:check:*'",
    "format:check:src": "prettier --check *.ts",
    "lint": "eslint --fix .",
    "lint:check": "run-s 'lint:check:*'",
    "lint:check:src": "eslint *.ts"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.33.6",
    "eslint": "8.28.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@types/psl": "1.1.0",
    "cross-env": "7.0.3",
    "psl": "1.9.0"
  },
  "packageManager": "yarn@3.2.4",
  "license": "UNLICENSED"
}
