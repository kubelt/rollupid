{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css --watch",
    "build": "remix build",
    "sass": "sass --watch app/:app/",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "concurrently \"npm run generate:css\" \"npm run sass\" \"remix build && run-p \"dev:*\"\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@datadog/browser-rum": "^4.19.0",
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "2.0.10",
    "@remix-run/cloudflare": "1.7.0",
    "@remix-run/cloudflare-workers": "1.7.0",
    "@remix-run/react": "1.7.0",
    "@tanstack/react-query": "4.2.3",
    "cross-env": "7.0.3",
    "ethers": "5.7.0",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.1.10",
    "history": "5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.4.0",
    "react-router-dom": "6.3.0",
    "wagmi": "0.6.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.1",
    "@remix-run/dev": "1.7.0",
    "@remix-run/eslint-config": "1.7.0",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "autoprefixer": "^10.4.8",
    "concurrently": "7.3.0",
    "eslint": "8.20.0",
    "miniflare": "2.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.16",
    "sass": "1.54.7",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4",
    "wrangler": "2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}
