{
  "name": "threeid",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "build": "run-s \"build:*\"",
    "build:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "build:sass": "sass app/:app/ --no-source-map",
    "build:remix": "remix build",
    "dev:sass": "sass --watch app/:app/",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler dev --local --persist",
    "dev": "echo \"No dev for threeid. Please use start.\"",
    "start": "run-p \"dev:*\"",
    "e2e": "cross-env NODE_ENV=test npx playwright test"
  },
  "dependencies": {
    "@datadog/browser-rum": "4.19.0",
    "@datadog/datadog-api-client": "^1.3.0",
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "2.0.10",
    "@remix-run/cloudflare": "1.7.0",
    "@remix-run/cloudflare-workers": "1.7.0",
    "@remix-run/react": "1.7.0",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/react-query": "4.2.3",
    "cross-env": "7.0.3",
    "ethers": "5.7.0",
    "flowbite": "1.5.3",
    "flowbite-react": "^0.1.10",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "graphql-tag": "2.12.6",
    "history": "5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.4.0",
    "react-infinite-scroll-component": "6.1.0",
    "react-masonry-css": "1.0.16",
    "react-router-dom": "6.3.0",
    "tw-elements": "1.0.0-alpha12",
    "wagmi": "0.6.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.1",
    "@playwright/test": "1.25.2",
    "@remix-run/dev": "1.7.0",
    "@remix-run/eslint-config": "1.7.0",
    "@remix-run/serve": "1.7.0",
    "@types/node": "18.7.18",
    "@types/puppeteer": "5.4.6",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "autoprefixer": "10.4.8",
    "concurrently": "7.3.0",
    "eslint": "8.20.0",
    "miniflare": "2.6.0",
    "postcss": "8.4.16",
    "sass": "1.54.7",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "wrangler": "2.1.13",
    "yarn-run-all": "3.1.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
