{
  "name": "console",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "run-s build:*",
    "build:css": "yarn run generate:css -- --minify",
    "build:remix": "remix build",
    "deploy": "wrangler publish",
    "deploy:dev": "wrangler publish --env dev",
    "dev:css": "yarn run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler dev --local --port 8989 --persist --env dev",
    "dev": "echo \"No dev for console. Please use start.\"",
    "start": "run-p \"dev:*\"",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@headlessui/react": "1.7.3",
    "@heroicons/react": "2.0.12",
    "@remix-run/cloudflare": "1.7.2",
    "@remix-run/cloudflare-workers": "1.7.2",
    "@remix-run/react": "1.7.2",
    "@tailwindcss/forms": "0.5.3",
    "bcryptjs": "2.4.3",
    "cross-env": "7.0.3",
    "ethers": "5.7.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "wagmi": "0.7.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.17.0",
    "@remix-run/dev": "1.7.2",
    "@remix-run/eslint-config": "1.7.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.25.0",
    "miniflare": "2.10.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4",
    "wrangler": "2.1.13",
    "yarn-run-all": "3.1.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
