{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["@cloudflare/workers-types"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": ".build",
    "skipLibCheck": true
  },
  "include": ["src"]
}
