{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --do-persist --kv-persist --wrangler-env development",
    "dev:css": "npm run generate:css -- --watch",
    "dev": "remix build && run-p \"dev:*\"",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@datadog/browser-rum": "4.21.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-hook/window-size": "3.1.1",
    "@remix-run/cloudflare": "1.7.2",
    "@remix-run/cloudflare-workers": "1.7.2",
    "@remix-run/react": "1.7.2",
    "cross-env": "7.0.3",
    "ethers": "5.7.1",
    "flowbite-react": "0.1.11",
    "react": "18.2.0",
    "react-confetti": "6.1.0",
    "react-countdown": "2.3.2",
    "react-dom": "18.2.0",
    "wagmi": "0.6.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.1",
    "@remix-run/dev": "1.7.2",
    "@remix-run/eslint-config": "1.7.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "17.0.17",
    "eslint": "8.23.1",
    "miniflare": "2.9.0",
    "npm-run-all": "4.1.5",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "wrangler": "2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}
