{
  "name": "@kubelt/platform.address",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:generate": "drizzle-kit generate --dialect=sqlite --schema=src/db/schema.ts",
    "check": "run-s format:check lint:check types:check",
    "format": "run-s format:src",
    "format:src": "prettier --write src",
    "format:check": "run-s format:check:src",
    "format:check:src": "prettier --check src",
    "lint": "eslint --fix src test",
    "lint:check": "run-s lint:check:src",
    "lint:check:src": "eslint src",
    "types:check": "tsc --project tsconfig.json",
    "dev": "wrangler dev --local --persist",
    "deploy": "wrangler publish",
    "test": "run-s check"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.18.0",
    "@types/itty-router-extras": "0.4.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "drizzle-kit": "0.14.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "wrangler": "2.4.4"
  },
  "dependencies": {
    "@ethersproject/address": "5.7.0",
    "@ethersproject/bytes": "5.7.0",
    "@ethersproject/keccak256": "5.7.0",
    "@ethersproject/random": "5.7.0",
    "@ethersproject/signing-key": "5.7.0",
    "@ethersproject/transactions": "5.7.0",
    "@kubelt/graph": "workspace:*",
    "@kubelt/openrpc": "workspace:*",
    "@kubelt/platform-clients": "workspace:*",
    "@kubelt/urns": "workspace:*",
    "drizzle-orm": "0.14.2",
    "drizzle-orm-sqlite": "0.14.2",
    "itty-router": "2.6.6",
    "itty-router-extras": "0.4.2",
    "moralis": "2.10.0"
  }
}
