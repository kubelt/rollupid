{
  "name": "@kubelt/platform.edges",
  "version": "0.1.0",
  "author": "ProofZero Inc.",
  "description": "Graph edge storage and retrieval",
  "private": true,
  "scripts": {
    "build:dev": "tsc",
    "db:local": "ts-node db/index.ts",
    "db:start": "run-s build:dev db:local",
    "db:orm": "typeorm-ts-node-commonjs",
    "dev": "run-s dev:local",
    "dev:local": "run-s types:check build:dev wrangler:local",
    "docs": "run-s build:* docs:*",
    "docs:build": "api-extractor run --local",
    "format": "run-s format:src",
    "format:src": "prettier --write **/*.ts",
    "format:check": "run-s format:check:*",
    "format:check:src": "prettier --check **/*.ts",
    "lint": "eslint --fix .",
    "lint:check": "run-s lint:check:*",
    "lint:check:src": "eslint *.ts",
    "test": "tap --ts test/",
    "types:check": "tsc --project tsconfig.json --noEmit",
    "wrangler:local": "wrangler dev --local --persist"
  },
  "tap": {
    "check-coverage": false
  },
  "devDependencies": {
    "@databases/split-sql-query": "1.0.3",
    "@databases/sql": "3.2.0",
    "@microsoft/api-extractor": "7.33.6",
    "@types/node": "18.11.9",
    "better-sqlite3": "8.0.0",
    "eslint": "8.28.0",
    "prettier": "2.7.1",
    "tap": "16.3.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "wrangler": "2.4.4",
    "yarn-run-all": "3.1.1"
  },
  "dependencies": {
    "@kubelt/openrpc": "workspace:*",
    "@kubelt/security": "workspace:*",
    "@kubelt/urns": "workspace:*",
    "cross-env": "7.0.3",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.1.2",
    "typeorm": "0.3.10",
    "urns": "0.6.0"
  },
  "packageManager": "yarn@3.2.4",
  "license": "UNLICENSED"
}
