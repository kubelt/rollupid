{
  "name": "@kubelt/platform.access",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "check": "run-s format:check lint:check types:check",
    "format": "run-s format:src",
    "format:src": "prettier --write src",
    "format:check": "run-s format:check:src",
    "format:check:src": "prettier --check src",
    "lint": "eslint --fix src test",
    "lint:check": "run-s lint:check:src",
    "lint:check:src": "eslint src",
    "types:check": "tsc --project tsconfig.json",
    "dev": "wrangler dev --local --persist",
    "deploy": "wrangler publish",
    "test": "run-s check"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.18.0",
    "@types/itty-router-extras": "0.4.0",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "wrangler": "2.4.4"
  },
  "dependencies": {
    "@ethersproject/bytes": "5.7.0",
    "@ethersproject/random": "5.7.0",
    "@kubelt/openrpc": "workspace:*",
    "@kubelt/platform.commons": "workspace:*",
    "@kubelt/urns": "workspace:*",
    "itty-router": "2.6.6",
    "itty-router-extras": "0.4.2",
    "jose": "4.11.0",
    "typed-json-rpc": "1.1.0"
  }
}
