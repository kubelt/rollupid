{
  "name": "@kubelt/services",
  "version": "0.1.0",
  "private": true,
  "useWorkspaces": true,
  "workspaces": [
    "*"
  ],
  "scripts": {
    "gql": "echo \"no gql configured for services\"",
    "services": "yarn workspaces foreach -i -v -p -j unlimited run services",
    "build": "yarn workspaces foreach -p -v -i run build",
    "test": "yarn workspaces foreach -p -v -i run test"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4"
}
